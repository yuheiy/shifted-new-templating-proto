{
	"name": "shifted-templating",
	"scripts": {
		"start": "concurrently --raw \"npm:start-eleventy\" \"vite\"",
		"start-eleventy": "rm -rf dist/ && npm run tsc-eleventy && concurrently --raw \"npm:eleventy -- --serve\" \"npm:tsc-eleventy -- --watch\"",
		"build": "vite build && npm run tsc-eleventy && ELEVENTY_ENV=production npm run eleventy",
		"eleventy": "NODE_OPTIONS=--enable-source-maps eleventy",
		"tsc-eleventy": "tsc --project tsconfig.eleventy.json"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.12.1",
		"@11ty/eleventy-navigation": "^0.2.0",
		"@types/node": "^15.12.2",
		"@types/react": "^17.0.9",
		"@types/react-dom": "^17.0.6",
		"concurrently": "^6.2.0",
		"prettier": "^2.3.1",
		"prettier-plugin-organize-imports": "^2.1.0",
		"sass": "^1.34.1",
		"type-fest": "^1.2.0",
		"typescript": "^4.3.2",
		"vite": "^2.3.7"
	},
	"engines": {
		"node": ">=16"
	},
	"private": true
}
