{
	"name": "shifted-templating",
	"scripts": {
		"start": "concurrently --raw \"npm:start:eleventy\" \"vite\"",
		"start:eleventy": "npm run start:eleventy:prepare && npm run start:eleventy:watch",
		"start:eleventy:prepare": "concurrently --raw \"rm -rf dist/\" \"tsc\"",
		"start:eleventy:watch": "concurrently --raw \"eleventy --serve\" \"tsc --watch\"",
		"build": "concurrently --raw \"vite build\" \"tsc\" && ELEVENTY_ENV=production eleventy"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.12.1",
		"@11ty/eleventy-navigation": "^0.2.0",
		"@types/node": "^15.12.1",
		"@types/react": "^17.0.9",
		"@types/react-dom": "^17.0.6",
		"concurrently": "^6.2.0",
		"prettier": "^2.3.0",
		"sass": "^1.34.1",
		"typescript": "^4.2.3",
		"vite": "^2.3.5"
	},
	"engines": {
		"node": ">=16"
	},
	"private": true
}
